{
  "name": "passport-headerapikey",
  "description": "Api key authentication strategy for Passport, which only handles headers (not body fields).",
  "keywords": [
    "apikey",
    "passport",
    "strategy"
  ],
  "author": "Christian Hotz <hotz@hydra-newmedia.com> ",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/hydra-newmedia/passport-headerapikey.git"
  },
  "license": "MIT",
  "main": "lib/index",
  "typings": "lib/index",
  "files": [
    "lib/"
  ],
  "scripts": {
    "prepublishOnly": "tsc",
    "install-typings": "typings install",
    "pretest": "tsc && rm -rf testDist/ && tsc --rootDir test --outDir testDist test/**/*.ts typings/index.d.ts",
    "test": "mocha testDist/* -R nyan --recursive"
  },
  "dependencies": {
    "express": "^4.14.0",
    "lodash": "^4.13.1",
    "passport": "^0.3.2",
    "passport-strategy": "^1.0.0"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "mocha": "^2.5.3",
    "sinon": "^1.17.4",
    "typescript": "^3.0.1",
    "typings": "^2.1.1"
  }
}
